{
    "name": "ayamel/transcoding-bundle",
    "type": "symfony-bundle",
    "description": "This bundle integrates the RabbitMQBundle and ACTranscodingBundle with the AyamelApiBundle to allow Resource object file uploads to be processed asynchronously.",
    "keywords": ["media", "resource", "transcoding"],
    "license": "MIT",
    "authors": [
        {
            "name": "Evan Villemez",
            "email": "evan.villemez@americancouncils.org"
        }
    ],
    "autoload": {
        "psr-0": { "Ayamel\\TranscodingBundle": "" }
    },
    "target-dir": "Ayamel/TranscodingBundle",
    "require": {
        "ayamel/api-bundle": "dev-master",
        "ac/transcoding-bundle": "dev-master",
        "oldsound/rabbitmq-bundle": "dev-master",
    }
}
