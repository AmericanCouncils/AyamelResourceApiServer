{
    "name": "ayamel/resource-library-api-server",
    "description": "An API server for hosting and sharing multimedia resources between applications and institutions.",
    "license": "MIT",
    "authors": [
        {
            "name": "Evan Villemez",
            "email": "evan.villemez@americancouncils.org"
        }
    ],
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "ext-apcu": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-pcntl": "*",
        "symfony/symfony": "2.3.*",
        "doctrine/mongodb-odm-bundle": "3.0.*@dev",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "ac/transcoding-bundle": "0.4.*",
        "oldsound/rabbitmq-bundle": "~1.1",
        "nelmio/api-doc-bundle": "2.3.*",
        "nelmio/cors-bundle": "1.1.*",
        "jms/serializer-bundle": "0.13.*",
        "ac/web-services-bundle": "0.3.*",
        "ac/media-info-bundle": "1.2.*",
        "friendsofsymfony/elastica-bundle": "dev-master",
        "ornicar/apc-bundle": "1.0.*",
        "ac/model-traits-bundle": "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "guzzle/guzzle": "~3.7",
        "fzaninotto/faker": "dev-master#991e39d280afc34a91684e5789023f1b276e4618"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin",
      "github-oauth":{
         "github.com":"4ea51a5e6d7241957ee2be481376bc06e1641447"
      }
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
